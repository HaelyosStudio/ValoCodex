{% extends 'base.html.twig' %}

{% block title %}User profile - {{ user.username }}{% endblock %}

{% block body %}
<div class="example-wrapper">
    <h1>User profile</h1>

    <dl>
        <dt>ID</dt>
        <dd>{{ user.id }}</dd>

        <dt>Username</dt>
        <dd>{{ user.username }}</dd>

        <dt>Email</dt>
        <dd>{{ user.email }}</dd>

        <dt>Wallet</dt>
        <dd>{{ user.wallet }}</dd>

        <dt>Agent</dt>
        <dd>{{ user.agent.name }}</dd>
    </dl>
</div>
{% endblock %}
